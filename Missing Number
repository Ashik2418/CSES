#include<bits/stdc++.h>
using namespace std;
# define int long long
#define opti ios_base::sync_with_stdio(false);
# define mise cin.tie(NULL);
signed main(){
opti mise
        int n,input,sum=0;
        cin>>n;
        for(int i=0;i<n-1;i++)
        {
            cin>>input;
            sum+=input;
        }
        cout<<(n*(n+1))/2-sum<<endl;
        return 0;


}
